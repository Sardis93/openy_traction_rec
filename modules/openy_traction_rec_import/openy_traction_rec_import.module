<?php

/**
 * @file
 * Contains openy_traction_rec_import.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function openy_traction_rec_import_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    case 'help.page.openy_traction_rec_import':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Salesforce migration for OPENY') . '</p>';
      return $output;

    default:
  }
}
